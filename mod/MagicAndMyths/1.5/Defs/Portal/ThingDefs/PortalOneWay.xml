<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="BuildingBase">
      <defName>Portal_Oneway</defName>
      <label>One Way Portal</label>
      <description>A Large stable portal gate capable of transporting pawns and things across massive distances.</description>
      <thingClass>MagicAndMyths.Building_OneWayPortal</thingClass>
      <drawerType>MapMeshAndRealTime</drawerType>
      <graphicData>
        <texPath>Things/Building/Misc/CaravanHitchingSpot</texPath>
        <graphicClass>Graphic_Single</graphicClass>
        <drawSize>(1,1)</drawSize>
      </graphicData>
      <altitudeLayer>Building</altitudeLayer>
      <passability>Standable</passability>
      <pathCost>0</pathCost>
      <fillPercent>0</fillPercent>
      <canOverlapZones>false</canOverlapZones>
      <tickerType>Normal</tickerType>
      <statBases>
        <WorkToBuild>2000</WorkToBuild>
        <MaxHitPoints>150</MaxHitPoints>
        <Flammability>1.0</Flammability>
      </statBases>
      <size>(1,1)</size>
      <costList>
        <Steel>1</Steel>
      </costList>
      <designationCategory>Misc</designationCategory>
      <placeWorkers>
        <li>PlaceWorker_NotUnderRoof</li>
      </placeWorkers>
      <building>
        <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
        <artificialForMeditationPurposes>false</artificialForMeditationPurposes>
      </building>
      <comps>
        <li Class="CompProperties_Forbiddable"/>
      </comps>
    </ThingDef>
</Defs>
