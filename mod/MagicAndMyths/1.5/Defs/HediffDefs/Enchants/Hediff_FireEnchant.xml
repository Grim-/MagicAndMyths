<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <HediffDef>
    <defName>FireEnchant</defName>
    <description>Fire Enchant</description>
    <label>Fire Enchant</label>
    <hediffClass>HediffWithComps</hediffClass>
    <comps>
      <li Class="MagicAndMyths.HediffCompProperties_Overlay">
        <overlayGraphic Class="MagicAndMyths.GraphicDataWithShader">
          <drawSize>1.1</drawSize>
          <texPath>Starscourgedual</texPath>
          <graphicClass>MagicAndMyths.Graphic_SingleWithShader</graphicClass>
          <customShaderName>Shader Forge/EnchantShader_Additive</customShaderName>
          <shaderParameters>
            <_OverlayTex>/noise/Noise94</_OverlayTex>
            <!-- <_OverlayMask></_OverlayMask> -->
            <_FireColor>( 3, 1.917, 0, 1)</_FireColor>
            <_Opacity>1</_Opacity>
            <_ShowEffect>1</_ShowEffect>
            <_OutlineOnly>1</_OutlineOnly>
            <_PanSpeedX>0</_PanSpeedX>
            <_PanSpeedY>-0.15</_PanSpeedY>
            <_FresnelColor>(1, 1, 1, 1)</_FresnelColor>
            <_FresnelExpo>0</_FresnelExpo>
            <_EdgeWidth>0.09</_EdgeWidth>
          </shaderParameters>
        </overlayGraphic>
      </li>
    </comps>
  </HediffDef>
</Defs>
