<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <HediffDef ParentName="DebuffBase">
    <defName>MagicAndMyths_Shocked</defName>
    <label>shocked</label>
    <description>This pawn has been struck by lightning. Electrical energy courses through their body, causing intense pain and slowing their movements.</description>
    <comps>
      <li Class="MagicAndMyths.HediffCompProperties_RemoveAfterTicks">
        <ticks>6800</ticks>
      </li>
    </comps>
    <stages>
      <li>
        <painFactor>1.6</painFactor>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.4</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>
